package Method_Overloading;

public class Methods {
	int a;
	int b;
	public void  sum (int a,int b)
	{
		a=a;
		b=b;
	}
	public int send()
	{
		return a+b;
	}
	

	public static void main(String[] args) {
	 Methods meth=new Methods();
	 meth.sum(10, 20);
	 int s=meth.send();
	 System.out.println("The sum is :"+s);
	}

}
